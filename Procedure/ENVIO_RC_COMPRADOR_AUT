CREATE OR REPLACE PROCEDURE ENVIO_RC_COMPRADOR_AUT (attr_ varchar2) is

   idRc_            VARCHAR2(3200) := NULL;
   id_projeto_         VARCHAR2(3200) := NULL;
   projeto_         VARCHAR2(3200) := NULL;
   idSite_          VARCHAR2(3200) := NULL;
   idComprador_     VARCHAR2(3200) := NULL;
   email_           VARCHAR2(3200) := NULL;
   usuRequisitante_ VARCHAR2(3200) := NULL;
   dataAutorizacao_ VARCHAR2(3200) := NULL;

   cabec_ CLOB := NULL;
   table_ CLOB := NULL;
   text_  CLOB := NULL;

CURSOR REQCOMPRA(v_id_comprador_ varchar2) IS
   SELECT pr.requisition_no ID_RC,
          pr.CF$_USER_REQUISITIONER USUARIO_REQ,
          pr.CONTRACT ID_SITE,
          ifsgft.PURCHASE_REQ_LINE_PART_API.get_BUYER_CODE(requisition_no_ => pr.requisition_no, line_no_ => 1, release_no_ => 1) ID_COMPRADOR,
--          ifsgft.COMM_METHOD_API.Get_Value('Person', ifsgft.PURCHASE_REQ_LINE_PART_API.get_BUYER_CODE(requisition_no_ => pr.requisition_no, line_no_ => 1, release_no_ => 1), 1) EMAIL_SUB,
          nvl((select B.VALUE from PERSON_INFO_COMM_METHOD2 B
                where B.identity = ifsgft.PURCHASE_REQ_LINE_PART_API.get_BUYER_CODE(requisition_no_ => pr.requisition_no, line_no_ => 1, release_no_ => 1)
                AND  B.METHOD_ID_DB = 'E_MAIL'
                AND  B.METHOD_DEFAULT = 'TRUE'
                AND ROWNUM=1),(SELECT C.VALUE FROM FND_USER_PROPERTY C
                                WHERE C.NAME = 'SMTP_MAIL_ADDRESS'
                                AND   C.identity = ifsgft.PURCHASE_REQ_LINE_PART_API.get_BUYER_CODE(requisition_no_ => pr.requisition_no, line_no_ => 1, release_no_ => 1) AND ROWNUM=1)) EMAIL_SUB,
          PURCHASE_REQ_LINE_PART_API.Get_Project_Id(requisition_no_ => pr.requisition_no, line_no_ => 1, release_no_ => 1) ID_PROJETO,
          Project_API.Get_Name(PURCHASE_REQ_LINE_PART_API.Get_Project_Id(requisition_no_ => pr.requisition_no, line_no_ => 1, release_no_ => 1)) PROJETO,
          DATE_APPROVED DATA_AUTORIZACAO
     FROM ifsgft.PURCHASE_REQUISITION_CFV pr
     JOIN  (SELECT REQUISITION_NO, MAX(DATE_APPROVED) DATE_APPROVED
             FROM PURCH_REQ_LINE_APPROVAL
              GROUP BY REQUISITION_NO) b   ON pr.REQUISITION_NO = b.REQUISITION_NO
    WHERE pr.OBJSTATE <> 'Partially Authorized'
     and DATE_APPROVED >= SYSDATE - INTERVAL '12' HOUR
     and ifsgft.PURCHASE_REQ_LINE_PART_API.get_BUYER_CODE(requisition_no_ => pr.requisition_no, line_no_ => 1, release_no_ => 1) = v_id_comprador_;

CURSOR REQCOMPRA_COMPRADOR IS
   SELECT distinct ifsgft.PURCHASE_REQ_LINE_PART_API.get_BUYER_CODE(requisition_no_ => pr.requisition_no, line_no_ => 1, release_no_ => 1) ID_COMPRADOR
     FROM ifsgft.PURCHASE_REQUISITION_CFV pr
     JOIN  (SELECT REQUISITION_NO, MAX(DATE_APPROVED) DATE_APPROVED
             FROM PURCH_REQ_LINE_APPROVAL
              GROUP BY REQUISITION_NO) b   ON pr.REQUISITION_NO = b.REQUISITION_NO
    WHERE pr.OBJSTATE <> 'Partially Authorized'
     and DATE_APPROVED >= SYSDATE - INTERVAL '12' HOUR;

CURSOR USUARIO_REQ(v_id_usuario_ varchar2) IS
   SELECT pr.requisition_no ID_RC,
          pr.CF$_USER_REQUISITIONER USUARI_REQ,
          nvl((select B.VALUE from PERSON_INFO_COMM_METHOD2 B
                where B.identity = pr.CF$_USER_REQUISITIONER
                AND  B.METHOD_ID_DB = 'E_MAIL'
                AND  B.METHOD_DEFAULT = 'TRUE'
                AND ROWNUM=1),(SELECT C.VALUE FROM FND_USER_PROPERTY C
                                WHERE C.NAME = 'SMTP_MAIL_ADDRESS'
                                AND   C.identity = pr.CF$_USER_REQUISITIONER)) EMAIL_SUB,
          pr.CONTRACT ID_SITE,
          ifsgft.PURCHASE_REQ_LINE_PART_API.get_BUYER_CODE(requisition_no_ => pr.requisition_no, line_no_ => 1, release_no_ => 1) ID_COMPRADOR,
--          ifsgft.COMM_METHOD_API.Get_Value('Person', ifsgft.PURCHASE_REQ_LINE_PART_API.get_BUYER_CODE(requisition_no_ => pr.requisition_no, line_no_ => 1, release_no_ => 1), 1) EMAIL_SUB,
          PURCHASE_REQ_LINE_PART_API.Get_Project_Id(requisition_no_ => pr.requisition_no, line_no_ => 1, release_no_ => 1) ID_PROJETO,
          Project_API.Get_Name(PURCHASE_REQ_LINE_PART_API.Get_Project_Id(requisition_no_ => pr.requisition_no, line_no_ => 1, release_no_ => 1)) PROJETO,
          DATE_APPROVED DATA_AUTORIZACAO
     FROM ifsgft.PURCHASE_REQUISITION_CFV pr
     JOIN  (SELECT REQUISITION_NO, MAX(DATE_APPROVED) DATE_APPROVED
             FROM PURCH_REQ_LINE_APPROVAL
              GROUP BY REQUISITION_NO) b   ON pr.REQUISITION_NO = b.REQUISITION_NO
    WHERE pr.OBJSTATE <> 'Partially Authorized'
     and DATE_APPROVED >= SYSDATE - INTERVAL '12' HOUR
     and pr.CF$_USER_REQUISITIONER = v_id_usuario_;

CURSOR REQCOMPRA_USUARIO IS
   SELECT distinct pr.CF$_USER_REQUISITIONER USUARI_REQ
     FROM ifsgft.PURCHASE_REQUISITION_CFV pr
     JOIN  (SELECT REQUISITION_NO, MAX(DATE_APPROVED) DATE_APPROVED
             FROM PURCH_REQ_LINE_APPROVAL
              GROUP BY REQUISITION_NO) b   ON pr.REQUISITION_NO = b.REQUISITION_NO
    WHERE pr.OBJSTATE <> 'Partially Authorized'
     and DATE_APPROVED >= SYSDATE - INTERVAL '12' HOUR;

BEGIN
transaction_sys.set_status_info('Entrando no primeiro loop.', 'INFO');
 FOR r IN REQCOMPRA_COMPRADOR
    LOOP
transaction_sys.set_status_info('Loop Id compprador: '||r.ID_COMPRADOR||'.', 'INFO');
        cabec_ := '<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Organização de Conteúdo</title>
    <style>
        body { font-family: Arial, sans-serif; margin: 0; padding: 0; overflow-x: hidden; }
        table { width: 100%; border-collapse: collapse; }
        th, td { padding: 8px; text-align: left; color: #333; }
        th { background-color: #f4f4f4; color: #333; }
        h3, h4 { margin-top: 0; color: #333; }
    </style>
</head>
<body>
    <table style="background-color: #ffffff;">
        <tr>
            <td style="padding: 15px;text-align:center;">
               <img alt="Descrição da Imagem" height="62" width="206" src="data:image/jpeg;base64,
















                                                                                     
















                                                                                     " data-imagetype="DataUri">
            </td>
        </tr>
        <tr>
            <td style="padding: 10px;">
                <h3 style="font-weight: normal;">Olá,</h3>
                <h3 style="font-weight: normal;">Abaixo o detalhamento das requisições de compra aprovadas:</h3>
            </td>
        </tr>
        <tr>
            <td style="padding: 10px;">
                <table>
                    <tr>
                        <th style="border:1px solid #ddd;">Requisição de Compra</th>
                        <th style="border:1px solid #ddd;">Usuário Requisitante</th>
                        <th style="border:1px solid #ddd;">Site</th>
                        <th style="border:1px solid #ddd;">Comprador</th>
                        <th style="border:1px solid #ddd;">Projeto</th>
                        <th style="border:1px solid #ddd;">Desc Projeto</th>
                    </tr>';

  -- Abre o cursor e percorre as requisições
  table_ := NULL;
  OPEN REQCOMPRA(r.ID_COMPRADOR);

  LOOP
    FETCH REQCOMPRA INTO idRc_, usuRequisitante_, idSite_, idComprador_, email_, id_projeto_, projeto_, dataAutorizacao_;
    EXIT WHEN REQCOMPRA%NOTFOUND;
transaction_sys.set_status_info('Loop dos dados: '||idRc_||'usureq: '||usuRequisitante_||'site: '||idSite_||'compra: '||idComprador_||'email: '||email_||'idproj: '||id_projeto_||'proj: '||projeto_||'dat: '||dataAutorizacao_||'.', 'INFO');
    -- Concatenar as requisições no corpo do e-mail (tabela)
    IF idRc_ IS NOT NULL THEN

      table_ := table_ || '<tr>
                            <td style="border:1px solid #ddd;">' || idRc_ || '</td>
                            <td style="border:1px solid #ddd;">' || usuRequisitante_ || '</td>
                            <td style="border:1px solid #ddd;">' || idSite_ || '</td>
                            <td style="border:1px solid #ddd;">' || idComprador_ || '</td>
                            <td style="border:1px solid #ddd;">' || id_projeto_ || '</td>
                            <td style="border:1px solid #ddd;">' || projeto_ || '</td>
                           </tr>';
    END IF;

  END LOOP;

--  CLOSE REQCOMPRA;

  -- Fechar o corpo do e-mail com a tabela concatenada
  cabec_ := cabec_ || table_ || '</table><br></td></tr></table></body></html>';

  -- Enviar o e-mail com todas as requisições
  command_sys.Mail(
    from_user_name_ => 'ifs@green4t.com',
    to_user_name_   =>  email_||';matheus.freitas@bismart.com.br',
    text_           => cabec_,
    subject_        => 'Requisições de Compra Aprovadas',
    from_alias_     => 'IFS'
  );
        commit;
  transaction_sys.set_status_info('E-mail enviado com sucesso.', 'INFO');
CLOSE REQCOMPRA;
 END LOOP;
    transaction_sys.set_status_info('E-mail enviado com sucesso para o comprador.', 'INFO');

       ------------------------------------ DISPARO PARA O USUARIO REQUISITANTE ------------------------------------------------------------
    BEGIN
            transaction_sys.set_status_info('Entrando no primeiro loop.', 'INFO');
            FOR r IN REQCOMPRA_USUARIO
                LOOP
                    transaction_sys.set_status_info('Loop Id usuario: '||r.USUARI_REQ||'.', 'INFO');
                    cabec_ := '<!DOCTYPE html>
<html lang="pt-br">
<head>
<meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Organização de Conteúdo</title>
    <style>
        body { font-family: Arial, sans-serif; margin: 0; padding: 0; overflow-x: hidden; }
        table { width: 100%; border-collapse: collapse; }
        th, td { padding: 8px; text-align: left; color: #333; }
        th { background-color: #f4f4f4; color: #333; }
        h3, h4 { margin-top: 0; color: #333; }
    </style>
</head>
<body>
    <table style="background-color: #ffffff;">
        <tr>
            <td style="padding: 15px;text-align:center;">
               <img alt="Descrição da Imagem" height="62" width="206" src="data:image/jpeg;base64,







qO9aXp3pheeqTs6zZdLTOd7koM0tbyGkt7+9ujKyMk7quAzzGLlU4nTYXDGal1s4aNBOoDcqrhWB12PSSNoWZ2Zr0gWve2vgVuvpUNbfe5KfvFj70aPnVhfxn8p7lNeYGPfKH5m96+L2UdbwhRTbUqVAHA74scT8+Poyqwu+l5/Ke5fDkBj9tEQ/M3vX2T2b9oJiWQz/ACKbG6DwNRkz0591j4/KLB3Ov4X/ABd3L7FkXlIxgb4D/Jn/AKXONnTaCJANmNDtNRlP/LHjzhwf5v2d3LJ5m5SfI/yZ/
                                                                                      








                                                                                      








                                                                                      







" data-imagetype="DataUri">
            </td>
        </tr>
        <tr>
            <td style="padding: 10px;">
                <h3 style="font-weight: normal;">Olá,</h3>
                <h3 style="font-weight: normal;">Abaixo o detalhamento das requisições de compra aprovadas:</h3>
            </td>
        </tr>
        <tr>
            <td style="padding: 10px;">
                <table>
                    <tr>
                        <th style="border:1px solid #ddd;">Requisição de Compra</th>
                        <th style="border:1px solid #ddd;">Usuário Requisitante</th>
                        <th style="border:1px solid #ddd;">Site</th>
                        <th style="border:1px solid #ddd;">Comprador</th>
                        <th style="border:1px solid #ddd;">Projeto</th>
                        <th style="border:1px solid #ddd;">Desc Projeto</th>
                    </tr>';

        -- Abre o cursor e percorre as requisições
            table_ := NULL;
        OPEN USUARIO_REQ(r.USUARI_REQ);
        LOOP
            FETCH USUARIO_REQ INTO idRc_, usuRequisitante_,email_ , idSite_, idComprador_, id_projeto_, projeto_, dataAutorizacao_;
            EXIT WHEN USUARIO_REQ%NOTFOUND;
            transaction_sys.set_status_info('Loop dos dados: '||idRc_||'usureq: '||usuRequisitante_||'site: '||idSite_||'compra: '||idComprador_||'email: '||email_||'idproj: '||id_projeto_||'proj: '||projeto_||'dat: '||dataAutorizacao_||'.', 'INFO');
        -- Concatenar as requisições no corpo do e-mail (tabela)
            IF idRc_ IS NOT NULL THEN

      table_ := table_ || '<tr>
                            <td style="border:1px solid #ddd;">' || idRc_ || '</td>
                            <td style="border:1px solid #ddd;">' || usuRequisitante_ || '</td>
                            <td style="border:1px solid #ddd;">' || idSite_ || '</td>
                            <td style="border:1px solid #ddd;">' || idComprador_ || '</td>
                            <td style="border:1px solid #ddd;">' || id_projeto_ || '</td>
                            <td style="border:1px solid #ddd;">' || projeto_ || '</td>
                           </tr>';
            END IF;

        END LOOP;

--  CLOSE REQCOMPRA;

  -- Fechar o corpo do e-mail com a tabela concatenada
  cabec_ := cabec_ || table_ || '</table><br></td></tr></table></body></html>';

    -- Enviar o e-mail com todas as requisições
        command_sys.Mail(
        from_user_name_ => 'ifs@green4t.com',
        to_user_name_   =>  email_||';matheus.freitas@bismart.com.br',
        text_           => cabec_,
        subject_        => 'Requisições de Compra Aprovadas',
        from_alias_     => 'IFS'
        );
        commit;
    transaction_sys.set_status_info('E-mail enviado com sucesso.', 'INFO');
    CLOSE USUARIO_REQ;
    END LOOP;
  transaction_sys.set_status_info('E-mail enviado com sucesso para o usuario requisitante.', 'INFO');
END;

END;
